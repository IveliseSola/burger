<h1>Eat a Burger</h1>

<h2>Burgers ready to devour!</h2>

<ul>
  {{#each burgers}}
    <li>
  	   <p>id: {{this.id}}</p>
       <p>burger_name: {{this.burger_name}}</p>
  	</li>
  {{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="burger"></textarea>

	<button type="submit">Submit</button>
</form>